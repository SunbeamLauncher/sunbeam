{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "additionalProperties": false,
    "properties": {
        "extensions": {
            "type": "object",
            "patternProperties": {
                ".+": {
                    "type": "string"
                }
            }
        },
        "items": {
            "type": "array",
            "items": {
                "type": "object",
                "additionalProperties": false,
                "required": [
                    "extension",
                    "command"
                ],
                "properties": {
                    "title": {
                        "type": "string"
                    },
                    "extension": {
                        "type": "string"
                    },
                    "command": {
                        "type": "string"
                    },
                    "params": {
                        "type": "object",
                        "patternProperties": {
                            ".+": {
                                "oneOf": [
                                    {
                                        "type": "string"
                                    },
                                    {
                                        "type": "boolean"
                                    }
                                ]
                            }
                        }
                    }
                }
            }
        }
    }
}
